{% if banner.banner_type == 1 %}
  <img src="{{ banner.media.banner_image.file_url }}"
       width="{{ banner.banner_place_obj.width }}" height="{{ banner.banner_place_obj.height }}" />
{% else %}
  <div style="width: {{ banner.banner_place_obj.width }}px; height: {{ banner.banner_place_obj.height }}px;">
    {{ banner.html }}
  </div>
{% endif %}
