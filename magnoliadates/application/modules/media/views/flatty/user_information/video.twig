<div class="content-page">
    <div class="ui-title">{% helper lang.l('header_main', 'media') %}</div>
    <div class="ui-descr">
        {% helper lang.l('field_usr_inf_description', 'media') %}
    </div>
</div>
{% for item in video %}
    <div class="col-xs-12 content-page">
        <div class="ui-title bb">{{ user.output_name }} {% helper lang.l('uploads_new_videos', 'media') %}</div>
        <div>
            {% if item.media_video == 'embed' %}
                {{ item.video_content.embed }}
            {% else %}
                <a href="video/{{ item.media_video }}">{{ item.fname }}</a>
            {% endif %}
        </div>
    </div>
{% endfor %}