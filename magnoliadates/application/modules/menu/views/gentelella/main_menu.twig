{% if auth_type == 'admin' %}
    {% for level1 in menu %}
        {% if level1.sub %}
            <div class="menu_section">
                <ul class="nav side-menu">
                    {% for level2 in level1.sub %}
                    <li {% if level2.active == 1 %} class="current-page"{% endif %}>
                        <a href="{{ level2.link }}" onclick="{% helper start.setAnalytics('left_menu', level2.gid) %}" 
                        {% if use_material_design_icons %} 
                            class="material-icons-link"
                        {% endif %}   
                            >
                            {% if use_material_design_icons %}    
                            <i class="material-icons">{{ level2.material_icon }}</i>   
                            <div class="menu_section_item-text">{{ level2.value }}</div>   
                            {% else %}  
                            <i class="fa fa-{{ level2.icon }}"></i> 
                            {{ level2.value }} 
                            {% endif %}  
                            {% if level2.indicator %}<span class="num">{{ level2.indicator }}</span>{% endif %}
                            
                        </a>
                    </li>
                    {% endfor %}
                </ul>
            </div>
        {% endif %}
    {% endfor %}
{% else %}
    <div class="menu">
        <div class="t">
            <div class="b min400">
            </div>
        </div>
    </div>
{% endif %}
